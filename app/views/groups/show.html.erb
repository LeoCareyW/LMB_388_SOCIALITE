
<div class="container mx-auto mt-4" style="width:300px; text-align: center">
  <h1><%= @group.name %></h1>
</div>

<div class="container">
  <div class="row">
    <div class="col-3">
      <% if @users.nil? %>
      <p>No members yet</p>
      <% else %>
      <% @users.each do |user| %>
      <%= user.name %>
      <% end %>
      <% end %>
